{
  "http": {
    "routers": {
      "gw-demo-traefik": {
        "rule": "PathPrefix(`/api/gw-demo-traefik`)",
        "service": "gw-demo-traefik",
        "entryPoints": ["web"],
        "middlewares": ["rate-limit"]
      }
    },
    "services": {
      "gw-demo-traefik": {
        "loadBalancer": {
          "servers": [
            { "url": "http://host.docker.internal:8080/" }
          ]
        }
      }
    },
    "middlewares": {
      "rate-limit": {
        "rateLimit": {
          "average": 5,
          "burst": 5,
          "period": "60s"
        }
      }
    }
  }
}
